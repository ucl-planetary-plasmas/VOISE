if [ -z "$1" ]; then
  echo Usage: $0 mcc
  exit 1
else
  MCC=$1
fi


FILES="startup start_VOISE addSeedsToVR addSeedToVD mergeVD benchVD parseArgs
computeHCThreshold plotHistHC computeVDFast plotVOISE computeVD prctile
divideVD printFigure exportfig printSeeds getCentroidSeed printSet
getCentroidVD printVD getDefaultVOISEParams readVOISEconf printVOISEsetup
getEquidistantPoint randomSeeds getVDOp removeSeedFromVD getVRclosure seedDist
getVRvertices homogeneousCriteria updateNeighboursListsFromVoronoin initVD
VOISE isXinW loadImage"

$MCC -R 'nodesktop,nodisplay' -m -v webVOISE $FILES

exit 0

$MCC -R 'nodesktop,nodisplay' -m -v webDemoVOISE $FILES

$MCC -R 'nodesktop,nodisplay' -m -v webDemoVOISE2 $FILES

